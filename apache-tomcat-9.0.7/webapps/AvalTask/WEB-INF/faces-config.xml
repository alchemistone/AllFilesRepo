<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>groupsBean1</managed-bean-name>
		<managed-bean-class>ua.aval.task.model.GroupsBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>funcsBean1</managed-bean-name>
		<managed-bean-class>ua.aval.task.model.FuncsBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>paramsBean1</managed-bean-name>
		<managed-bean-class>ua.aval.task.model.ParamsBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>groupsView</managed-bean-name>
		<managed-bean-class>ua.aval.task.views.GroupsView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>funcsView</managed-bean-name>
		<managed-bean-class>ua.aval.task.views.FuncsView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>paramsView</managed-bean-name>
		<managed-bean-class>ua.aval.task.views.ParamsView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>validationView</managed-bean-name>
		<managed-bean-class>ua.aval.task.views.ValidationView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

</faces-config>
